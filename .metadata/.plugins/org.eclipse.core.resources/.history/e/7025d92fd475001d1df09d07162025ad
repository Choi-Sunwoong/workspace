package edu.kh.variable.practice;

public class CastingPractice1 {

	public static void main(String[] args) {
		int iNum1 = 10;
		int iNum2 = 4;
		double iNum3 = iNum1;
								
		float fNum = 3.0f;
		int fNum1 = (int) fNum;
				
		double dNum = 2.5;
		int dNum1 = (int) dNum;
	

		char ch = 'A';
		int ch1 = ch;
		
	
		
		System.out.println(iNum1/iNum2); // 2			
		System.out.println(dNum1); // 2
		
		System.out.println(iNum2*dNum); // 10.0
		System.out.println(iNum3); // 10.0
		
		System.out.println(iNum3/iNum2); // 2.5
		System.out.println((dNum)); // 2.5
		
		System.out.println(fNum1); // 3
		System.out.println(iNum1/fNum1); // 3
		
		System.out.println(iNum1/fNum);// 3.3333333
		System.out.println(iNum3/fNum); // 3.3333333333333335
		
		System.out.println(ch); // 'A'
		System.out.println(ch1); // 65
		System.out.println(ch+iNum1); // 75
		System.out.println((char)(ch+iNum1)); // 'K'			


	}

}
